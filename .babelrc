{
	"compact": false,
	"sourceMaps": "both",
	"env": {
		"development": {
			"presets": [
				["env", {
					"targets": {
						"browsers": ["> 1%", "last 2 versions"]
					},
					"modules": false
				}],
				"stage-1"
			],
			"plugins": [
				"external-helpers"
			]
		},
		"test": {
			"presets": [
				["env", {
					"targets": {
						"node": "current"
					}
				}],
				"stage-1"
			]
		}
	}
}
